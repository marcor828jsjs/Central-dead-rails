-- Script Central: carrega e executa os 3 scripts externos

-- Lista de URLs dos scripts
local scripts = {
    "https://raw.githubusercontent.com/marcor828jsjs/3-pessoa/main/3%C2%B0%20pessoa",
    "https://raw.githubusercontent.com/marcor828jsjs/Gui-dead/refs/heads/main/Gui%20dead%20rails",
    "https://raw.githubusercontent.com/marcor828jsjs/Ilumina-o-eterna/refs/heads/main/Ilumina%C3%A7%C3%A3o%20dead%20rails"
}

-- Função para carregar todos os scripts
for _, url in ipairs(scripts) do
    pcall(function()
        local scriptContent = game:HttpGet(url)
        loadstring(scriptContent)()
    end)
end
